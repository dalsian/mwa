<h1>{{title}}</h1>
<div>
<table>
    <tr>
        <th>Location Name</th>
        <th>Category</th>
        <th>Longitude</th>
        <th>Latitude</th>
        <th></th>
    </tr>
    {{#each locations}}
        <tr>
            <td>{{name}}</td>
            <td>{{category}}</td>
            <td>{{map.coordinates.[0]}}</td>
            <td>{{map.coordinates.[1]}}</td>
            <td></td>
        </tr>
    {{/each}}
</table>
<a href="locations/add">Add Location</a><br />
<a href="locations/search">View Nearest Places</a>
</div>
